{
  "name": "examplejs",
  "version": "0.0.0",
  "description": "how to setup a react webapp on pythonanywhere",
  "main": "bundled.js",
  "scripts": {
    "test": "jest --noStackTrace --no-cache --bail",
    "retest": "jest --no-cache --watch=all",
    "build": "webpack --config webpack.config.js --progress --profile --colors && sass src/main.scss | uglifycss > ../static/bundled.css"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/react-redux/",
      "<rootDir>/node_modules/redux/",
      "<rootDir>/node_modules/redux-form/",
      "<rootDir>/node_modules/redux-logger/",
      "<rootDir>/node_modules/ramda/",
      "<rootDir>/src/testHelpers.js"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": ".*\\.spec\\.js$",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/conradho/examplejs.git"
  },
  "author": "conrad",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/conradho/examplejs/issues"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^12.1.1",
    "ramda": "^0.21.0",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-form": "^5.2.4",
    "redux-logger": "^2.6.1",
    "sass-loader": "^3.2.0",
    "uglifycss": "0.0.20",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.13.1"
  }
}
