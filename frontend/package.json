{
  "name": "examplejs",
  "version": "0.0.0",
  "description": "how to setup a react webapp on pythonanywhere",
  "main": "bundled.js",
  "scripts": {
    "test": "jest --watch=all --noStackTrace",
    "retest": "jest --no-cache --bail",
    "build": "browserify -t [ babelify --presets [ react es2015 ] ] src/app.jsx | uglifyjs > ../static/bundled.js && sass src/main.scss | uglifycss > ../static/bundled.css",
    "js": "browserify -t [ babelify --presets [ react es2015 ] ] src/app.jsx -o ../static/bundled.js",
    "css": "sass --watch src/main.scss:../static/bundled.css"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testDirectoryName": [
      "components"
    ],
    "testFileExtensions": [
      "spec.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/conradho/examplejs.git"
  },
  "author": "conrad",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/conradho/examplejs/issues"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^11.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "jest-cli": "^11.0.0",
    "ramda": "^0.21.0",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "uglifycss": "0.0.20",
    "uglifyjs": "^2.4.10"
  }
}
